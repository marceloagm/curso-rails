<%= @aposta.inspect %>



 <div class="card-block">
            <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
            <div class="x_title">
            <h2 style="text-align: center;">Dados da Equipe </h2>
            <ul class="nav navbar-right panel_toolbox">
                                             
            </ul>
            <div class="clearfix"></div>
            </div>
            <div class="x_content">
           
                <%= form_with(url: admins_backoffice_resultados_path, model: @aposta, local: true, class:"form-horizontal form-label-left") do |form| %>
                    <div class="form-group row">
                            <%= form.label :id, class:"col-sm-2 col-form-label" %>
                            <div class="col-sm-10">
                            <%= form.text_field :id, class:"form-control",readonly:true %>
                            </div>
                    </div>

                    <div class="form-group row">
                            <%= form.label :rodada, class:"col-sm-2 col-form-label" %>
                            <div class="col-sm-10">
                            <%= form.text_field :rodada, class:"form-control" %>
                            </div>
                    </div>

                    <div class="form-group row">
                            <%= form.label :equipe_nome, class:"col-sm-2 col-form-label" %>
                            <div class="col-sm-10">
                            <%= form.text_field :equipe_nome, class:"form-control", readonly:true %>
                            </div>
                    </div>
                    <%= form.hidden_field :rodada_antiga, :value => params["rodada"] %>

                    <div class="ln_solid"></div>
                    <div class="form-group">
                    <div style="text-align: center;"class="">
                        <button type="submit" class="btn btn-success">Salvar</button>
                    </div>
                    </div>

            <% end %>
            </div>
        </div>
        </div>
    </div>


